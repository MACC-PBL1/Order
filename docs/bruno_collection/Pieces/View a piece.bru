meta {
  name: View a piece
  type: http
  seq: 2
}

get {
  url: {{baseURL}}/piece/{{pieceId}}
  body: none
  auth: inherit
}

assert {
  res.status: eq 200
  res.body: isJson
  res.body.order: isJson
  res.body.id: isNumber
  res.body.id: eq {{pieceId}}
  res.body.manufacturing_date: isDefined
  res.body.status: isNotEmpty
}

settings {
  encodeUrl: true
}
